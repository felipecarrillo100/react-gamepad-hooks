{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,

    // Add these for unused variable/parameter warnings
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": ["src"]
}
